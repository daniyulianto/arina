<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="Quota.Dashboard">
        <div class="qd_container">
            <!-- <div class="row"> -->
                <div class="col-sm-12">
                    <table class="table table-borderless" style="border-collapse: separate; border-spacing: 5px;">
                        <tbody>
                            <tr>
                                <td style="text-align: center; font-weight: bold; vertical-align: middle;">Active</td>
                                <td class="qd_column_val qd_open_action" action="asb_quota.quota_quota_action"><t t-esc="active.total"/><br/><span>Total Active</span></td>
                                <td class="qd_column_val"><t t-esc="active.kontrak"/><br/><span>Kontrak</span></td>
                                <td class="qd_column_val"><t t-esc="active.magang"/><br/><span>Magang</span></td>
                                <td class="qd_column_val"><t t-esc="active.permanent"/><br/><span>Permanent</span></td>
                            </tr>
                            <tr>
                                <td style="text-align: center; font-weight: bold; vertical-align: middle;">Hold</td>
                                <td class="qd_column_val qd_open_action" action="asb_quota.quota_quota_action"><t t-esc="hold.total"/><br/><span>Total Hold</span></td>
                                <td class="qd_column_val"><t t-esc="hold.kontrak"/><br/><span>Kontrak</span></td>
                                <td class="qd_column_val"><t t-esc="hold.magang"/><br/><span>Magang</span></td>
                                <td class="qd_column_val"><t t-esc="hold.permanent"/><br/><span>Permanent</span></td>
                                
                            </tr>
                            <tr>
                                <td style="text-align: center; font-weight: bold; vertical-align: middle;">Done</td>
                                <td class="qd_column_val qd_open_action" action="asb_quota.quota_quota_action"><t t-esc="done.total"/><br/><span>Total Done</span></td>
                                <td class="qd_column_val"><t t-esc="done.kontrak"/><br/><span>Kontrak</span></td>
                                <td class="qd_column_val"><t t-esc="done.magang"/><br/><span>Magang</span></td>
                                <td class="qd_column_val"><t t-esc="done.permanent"/><br/><span>Permanent</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            <!-- </div> -->
        </div>
    </t>
</templates>